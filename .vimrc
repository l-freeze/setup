set visualbell t_vb=
set belloff=all
colorscheme molokai
set encoding=utf-8
"set fileencodings=iso-2022-jp,euc-jp,sjis,utf-8
set tabstop=4
set shiftwidth=4
set expandtab
"set autoindent
set cindent
set hlsearch
set ignorecase
set smartcase
"set maxmem=4000
set maxmempattern=4096
"set maxmempattern=2000000
set backspace=2
set showmatch
"set list
set ruler
set nu
set viminfo='20,\"1000
set autoread
nmap <UP> gk
nmap <DOWN> gj
vmap <UP> gk
vmap <DOWN> gj
set virtualedit=block
syntax on
filetype plugin indent on
"
